#include<iostream>
using namespace std;
void h(int f1,int f2,int jjak, int a[]) //f1친구1,f2=친구2,jjak=남은짝수,a[]=짝확인
{
    
}
int main(void)
{
    int c,n,m; //c=testcase,n=학생수,m=친구쌍수
    cin>>c;
    int a[c][2],b[c+1]={0,}; //a[]=친구관계정리,b[]=짝 지어졌는지
    for(int i=0; i<c; i++)
    {
        cin>>n>>m;
        for(int z=0; z<m; z++)
        {
            cin>>a[z][0]>>a[z][1];
        }
        for(int i=0; i<m; i++)
        {
            b[a[i][0]]=b[a[i][1]]=1;
            for(auto i:a)
                cout<<i<<'\n';
            //h(a[i][0],a[i][1],n/2,b);
        }
    }
}
//미완
