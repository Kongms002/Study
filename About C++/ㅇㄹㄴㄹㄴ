#include <iostream>
using namespace std;
int main()
{
	int n,a[200001]={0,},b[200001]={0,},x=0;
	cin>>n;
	for(int i=0; i<n; i++)
	{
		int k;
		cin>>k;
		if(k>0)
			a[k]=1;
		else
		{
			k*=-1;
			b[k]=1;
		}
	}
	for(int i=1; i<200001; i++)
	{
		if(a[i]==1&&b[i]==0)
			x+=i;
		else if(a[i]==0&&b[i]==1)
			x-=i;
	}
	cout<<x;
}
